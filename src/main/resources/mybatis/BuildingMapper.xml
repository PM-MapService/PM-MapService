<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.capstone.team5.repository.BuildingRepository">

    <select id="selectByName"
            resultType="BuildingEntity">
        SELECT building_id, name, latitude, longitude
          FROM building
         WHERE name = #{name}
    </select>
